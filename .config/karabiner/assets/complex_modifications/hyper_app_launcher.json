{
  "$schema": "https://ale.sh/karabiner-jsonschema.json",
  "title": "Hyper Key App Launchers [external and built-in keyboards]",
  "rules": [
    {
      "description": "Launch apps with Hyper + key [external and built-in keyboards] with 'o' held",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "o",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_control",
                "left_option"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "o_held",
                "value": 1
              }
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "o_held",
                "value": 0
              }
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "z",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_control",
                "left_option"
              ]
            }
          },
          "conditions": [
            {
              "type": "variable_if",
              "name": "o_held",
              "value": 1
            }
          ],
          "to": [
            {
              "shell_command": "open -a 'Zen'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_control",
                "left_option"
              ]
            }
          },
          "conditions": [
            {
              "type": "variable_if",
              "name": "o_held",
              "value": 1
            }
          ],
          "to": [
            {
              "shell_command": "open -a 'WezTerm'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "g",
            "modifiers": {
