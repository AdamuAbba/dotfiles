{
  "title": "Hyper Key App Launchers",
  "rules": [
    {
      "description": "Launch apps with Hyper + key",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "z",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_control",
                "left_option"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Zen'"
            }
          ]
        },
        {
          "type": "basic",
