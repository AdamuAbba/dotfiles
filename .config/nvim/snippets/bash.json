{
  "Bash Function Documentation": {
    "prefix": "docf",
    "body": [
      "# Function: ${1:function_name}",
      "# -------------------------",
      "# Description:",
      "#   ${2:Description of the function}",
      "# Parameters:",
      "#   $1 - ${3:Description of parameter 1}",
      "# Returns:",
      "#   ${4:Description of what the function returns}",
      "# Usage:",
      "#   ${5:enter usage}",
      "",
      "function ${1:function_name}() {",
      "    ${6:# Function logic goes here}",
      "}",
      ""
    ],
    "description": "Generate documentation for a Bash function"
  },
  "Add separator": {
    "prefix": [
      "divider"
    ],
    "body": "#============================================= $0 ============================================="
  },
  "Comment Divider": {
    "prefix": "comment",
    "body": [
      "#################################",
      "#        {1:Comment}            #",
      "#################################"
    ],
    "description": "Insert a comment divider"
  }
}
