{
  "markdownlint": {
    "prefix": ";markdownlint",
    "body": [
      " ",
      "<!-- markdownlint-disable -->",
      " ",
      "> $1",
      " ",
      "<!-- markdownlint-restore -->"
    ],
    "description": "Markdownlint disable range"
  },
  "prettier ignore": {
    "prefix": ";prettier-ignore",
    "body": [
      " ",
      "<!-- prettier-ignore-start -->",
      " ",
      "> $1",
      " ",
      "<!-- prettier-ignore-end -->"
    ],
    "description": "Prettier ignore range"
  },
  "regular link": {
    "prefix": ";link",
    "body": [
      "[$1]($2)"
    ],
    "description": "Create a new link"
  },
  "link in new tab": {
    "prefix": ";linkt",
    "body": [
      "[$1]($2){:target=\"_blank\"}"
    ],
    "description": "Open a link in a new tab (chirpy theme)"
  },
  "cl": {
    "body": "<!-- column_layout: [$0,$1] -->",
    "prefix": ";cll"
  },
  "cl-1": {
    "body": "<!-- column: $0 -->",
    "prefix": ";cl"
  },
  "e_s": {
    "body": "<!-- end_slide -->",
    "prefix": ";e_s"
  }
}
